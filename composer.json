{
    "name": "sinnbeck/laravel-langcheck",
    "description": "Find missing translations",
    "keywords": [
        "laravel",
        "i18n",
        "translation",
        "language",
        "trans"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ren√© Sinnbeck",
            "email": "rene.sinnbeck@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.2",
        "illuminate/container": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/contracts": "^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sinnbeck\\LangCheck\\": "src",
            "Sinnbeck\\LangCheck\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sinnbeck\\LangCheck\\LangCheckServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
